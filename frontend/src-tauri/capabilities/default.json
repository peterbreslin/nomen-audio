{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-destroy",
    "dialog:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://*" },
        { "url": "http://*:*" }
      ]
    }
  ]
}
